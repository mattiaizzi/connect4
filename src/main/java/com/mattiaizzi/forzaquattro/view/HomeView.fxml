<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" styleClass="background" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mattiaizzi.forzaquattro.view.HomeController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="FORZA QUATTRO">
         <styleClass>
            <String fx:value="label-bright" />
            <String fx:value="label-header" />
         </styleClass>
      </Label>
      <Button fx:id="play" layoutX="14.0" layoutY="160.0" mnemonicParsing="false" onAction="#handlePlay" text="Play" />
      <Button fx:id="info" layoutX="14.0" layoutY="238.0" mnemonicParsing="false" onMouseEntered="#handleInfo" onMouseExited="#outOfInfo" text="Info" />
      <Button fx:id="exit" layoutX="15.0" layoutY="314.0" mnemonicParsing="false" onAction="#handleExit" text="Exit" />
      <Pane fx:id="animationPane" layoutX="173.0" layoutY="104.0" prefHeight="300.0" prefWidth="400.0" />
   </children>
</AnchorPane>
